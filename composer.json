{
    "name": "sweeeeep/l5popularity",
    "description": "Laravel Package for Tracking popular item in Eloquent models",
    "license": "MIT",
    "authors": [
        {
            "name": "Raeywen Regachuelo",
            "email": "raeywenregach@gmail.com",
            "homepage": "http://sweeeeep.github.io"
        }
    ],
    "homepage": "https://github.com/sweeeeep/popularity",
    "keywords": ["Laravel", "Popularity"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "classmap": [
            "database/Migrations"
        ],
        "psr-4": {
            "Sweeeeep\\Popularity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweeeeep\\Popularity\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sweeeeep\\Popularity\\PopularityServiceProvider"
            ],
            "aliases": {
                "Popularity": "Sweeeeep\\Popularity\\Facades\\Popularity"
            }
        }
    }
}